    "json-stable-stringify": "^1.3.0",
    "openai": "^4.92.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
